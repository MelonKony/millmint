* {
  box-sizing: border-box;
  max-width: 100vw;
}

html {
  color: var(--font-color, var(--gray-dark, #262626));
  overflow-y: scroll;
  overflow-x: hidden;
  position: relative;
  scroll-behavior: smooth;
  width: 100%;
  font-family: -apple-system, BlinkMacSystemFont, Helvetica Neue, Helvetica, sans-serif, 'Emoji', apple color emoji;
  font-size: 16px;
  line-height: 1.5;
}

body {
  background-color: var(--body-background);
  position: relative;
  width: 100%;
  overflow-x: hidden;
  margin: 0;
  padding: 0;
}

main {
  margin-left: auto;
  margin-right: auto;
  margin-top: 1rem;
  margin-bottom: 1rem;
  padding: 1.5rem;
  border-radius: 0.25rem;
  background-color: var(--main-background, #fff);
  box-sizing: border-box;
  max-width: 45rem;
  width: 100%;
  overflow-x: hidden;
}

figure {
  margin: 0;
}

figure figcaption {
  font-size: 0.75rem;
  line-height: 1rem;
  font-weight: 600;
  padding: 0.5rem;
  border-radius: 0.25rem;
  background-color: var(--highlight-background, var(--gray-light));
  color: var(--highlight, var(--gray-medium));
  margin: -1rem 0 0.5rem 0;
  text-align: center;
  letter-spacing: -.01em;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
}

figure figcaption a {
  text-decoration: underline;
}

sub, sup {
  font-size: 0.875rem;
  line-height: 1.25rem;
  border-radius: 0.25rem;
  top: -0.5em;
  background-color: var(--body-background);
  padding: 1px 4px;
}

input, video {
  border-radius: 0.25rem;
  width: 100%;
}

footer {
  margin: 1rem 0 0 0;
  width: 100%;
}

hr {
  margin-top: 2rem;
  margin-bottom: 2rem;
  border: 1px solid var(--gray-medium);
}

.hidden {
  display: none;
}

::placeholder {
  font-size: 14px;
  color: var(--highlight, var(--gray-medium));
}

mark {
  background-color: var(--highlight-background, var(--gray-light));
  font-weight: 500;
}

details {
  margin-top: 0.5rem;
  margin-bottom: 0.5rem;
  font-size: 0.875rem;
  line-height: 1.25rem;
  border-radius: 0.25rem;
  background-color: var(--highlight-background, var(--gray-light));
  border: 1px solid var(--highlight, var(--gray-medium));
  padding: 0.5rem 1rem;
  outline: none;
}

details div {
  padding-left: 1.5rem;
  padding-right: 1.5rem;
}

details table {
  margin-top: 1.5rem;
  margin-bottom: 1.5rem;
  border-radius: 0.25rem;
  border: 1px solid var(--highlight, var(--gray-medium));
  font-size: 0.875rem;
  line-height: 1.25rem;
  width: 100%;
  border-spacing: 0;
  table-layout: auto;
  border: 1px solid var(--highlight-background, var(--gray-light));
}

@media screen and (max-width: 640px) {
  details table {
    overflow-x: auto;
  }
}

details table tr:nth-child(even) {
  background-color: var(--highlight-background, var(--gray-light));
}

details table td, details table th {
  padding-left: 0.75rem;
  padding-right: 0.75rem;
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
  border: 1px solid var(--highlight-background, var(--gray-light));
  border-radius: 0;
  width: auto;
  word-wrap: break-word;
  white-space: normal;
}

details table td:first-child, details table th:first-child {
  border-left-width: 1px;
}

details table td:last-child, details table th:last-child {
  border-right-width: 1px;
}

details table thead tr:first-child th:first-child {
  border-top-left-radius: 0.25rem;
}

details table thead tr:first-child th:last-child {
  border-top-right-radius: 0.25rem;
}

details table tbody:first-child tr:first-child td:first-child {
  border-top-left-radius: 0.25rem;
}

details table tbody:first-child tr:first-child td:last-child {
  border-top-right-radius: 0.25rem;
}

details table tbody:last-child tr:last-child td:first-child {
  border-bottom-left-radius: 0.25rem;
}

details table tbody:last-child tr:last-child td:last-child {
  border-bottom-right-radius: 0.25rem;
}

details table td, details table th {
  border: 1px solid var(--highlight-background, var(--gray-light));
}

details > summary {
  padding-left: 0.5rem;
  padding-right: 0.5rem;
  font-weight: 700;
  list-style: none;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

summary::-webkit-details-marker {
  display: none;
}

summary::after {
  content: ' ►';
  float: right;
  width: 20px;
}

details[open] summary:after {
  content: " ▼";
}

