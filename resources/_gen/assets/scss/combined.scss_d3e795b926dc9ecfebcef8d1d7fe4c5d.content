}

.postcard-layout .card figure {
  margin: 0;
}

.postcard-layout .card a {
  text-decoration: none;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.postcard-layout .card .postcard-meta {
  position: relative;
  z-index: 3;
}

.postcard-layout .card .postcard-meta .title {
  padding-left: 0.25rem;
  padding-right: 0.25rem;
  margin-top: 0;
  margin-bottom: 0;
  font-size: 1.125rem;
  line-height: 1.75rem;
  font-weight: 600;
  color: var(--font-color, var(--gray-dark, #262626));
  line-height: 1.2;
  font-weight: 500;
}

.postcard-layout .card .postcard-image {
  flex: 1;
  min-height: 0;
  padding: 0;
  border-radius: 0.25rem;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  overflow: hidden;
  background: black;
  z-index: 1;
  position: relative;
}

.postcard-layout .card .postcard-image img {
  margin-top: 0;
  margin-bottom: 0;
  position: absolute;
  top: 0;
  height: 360px !important;
  object-fit: cover !important;
  object-position: top !important;
  transform: scale(1.1) !important;
  width: 100%;
}

.postcard-layout .card:hover {
  border: 2px solid var(--highlight, #EA9365);
  box-shadow: 0px 5px 10px 2.5px var(--highlight-background, rgba(234, 147, 101, 0.08));
  transform: translateY(-0.5rem);
}

.postcard-layout .postcard-meta {
  padding: 0.5rem;
  border-radius: 0.25rem;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
  overflow: hidden;
}

.postcard-layout .postcard-meta .date {
  font-size: 0.875rem;
  line-height: 1.25rem;
  color: var(--gray-heavy);
  padding: 0px 10px 5px 5px;
}

.controls-row.note {
  background: var(--highlight-background, var(--gray-light));
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}

.checkbox-wrapper * {
  box-sizing: border-box;
  margin: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 0.5rem;
}

.checkbox-wrapper .cbx {
  -webkit-user-select: none;
  user-select: none;
  cursor: pointer;
  padding: 6px 8px;
  border-radius: 6px;
  overflow: hidden;
  transition: all 0.2s ease;
  display: inline-block;
}

.checkbox-wrapper .cbx:not(:last-child) {
  margin-right: 6px;
  width: max-content;
  display: flex;
}

.checkbox-wrapper .cbx:hover {
  background-color: var(--highlight-background, var(--gray-light));
}

.checkbox-wrapper .cbx span {
  float: left;
  vertical-align: middle;
  transform: translate3d(0, 0, 0);
}

.checkbox-wrapper .cbx span:first-child {
  position: relative;
  width: 18px;
  height: 18px;
  border-radius: 4px;
  transform: scale(1);
  border: 1px solid var(--highlight);
  transition: all 0.2s ease;
  box-shadow: 0 1px 1px var(--highlight-background);
}

.checkbox-wrapper .cbx span:first-child svg {
  position: absolute;
  top: 3px;
  left: 2px;
  fill: none;
  stroke: var(--main-background);
  stroke-width: 2;
  stroke-linecap: round;
  stroke-linejoin: round;
  stroke-dasharray: 16px;
  stroke-dashoffset: 16px;
  transition: all 0.3s ease;
  transition-delay: 0.1s;
  transform: translate3d(0, 0, 0);
}

.checkbox-wrapper .cbx span:last-child {
  padding-left: 8px;
  line-height: 18px;
}

.checkbox-wrapper .cbx:hover span:first-child {
  border-color: var(--highlight);
}

.checkbox-wrapper .inp-cbx {
  position: absolute;
  visibility: hidden;
}

.checkbox-wrapper .inp-cbx:checked + .cbx span:first-child {
  background: var(--highlight);
  border-color: var(--highlight);
  animation: wave 0.4s ease;
}

.checkbox-wrapper .inp-cbx:checked + .cbx span:first-child svg {
  stroke-dashoffset: 0;
}

.checkbox-wrapper .inline-svg {
  position: absolute;
  width: 0;
  height: 0;
  pointer-events: none;
  user-select: none;
}

@media screen and (max-width: 640px) {
  .checkbox-wrapper .cbx {
    width: 100%;
    display: inline-block;
  }
}

@-moz-keyframes wave {
  50% {
    transform: scale(0.9);
  }
}

@-webkit-keyframes wave {
  50% {
    transform: scale(0.9);
  }
}

@-o-keyframes wave {
  50% {
    transform: scale(0.9);
  }
}

@keyframes wave {
  50% {
    transform: scale(0.9);
  }
}

.pagination {
