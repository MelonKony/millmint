}

.postcard-layout .postcard-grid {
  padding: 0;
  margin-top: 1.5rem;
  margin-bottom: 1.5rem;
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 10px;
  grid-auto-flow: row;
  list-style: none;
  padding: 0;
}

@media (max-width: 1024px) {
  .postcard-layout .postcard-grid {
    grid-template-columns: repeat(2, 1fr);
  }
}

@media (max-width: 640px) {
  .postcard-layout .postcard-grid {
