  grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));
  gap: 10px 10px;
  grid-auto-flow: row;
  grid-auto-rows: 1fr; }

.shop-card h1, .shop-card h2, .shop-card h3, .shop-card a, .shop-card p {
