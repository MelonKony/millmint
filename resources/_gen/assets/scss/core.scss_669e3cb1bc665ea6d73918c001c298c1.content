  align-self: center; }

.search {
  position: relative;
  justify-items: end;
  border-bottom: 1px solid transparent;
  min-width: 10rem;
  grid-area: search; }
  .search input {
    background-color: var(--highlight-background, var(--gray-light));
    border: 1px solid var(--highlight, var(--gray-medium));
    width: 100%;
    height: 35px;
    padding: 2px;
    box-sizing: border-box;
    border-radius: 5px;
    color: var(--color-gray); }
    .search input:required + .search-spinner {
      display: block; }
  .search .search-spinner {
    position: absolute;
    top: 0;
    margin: 8px;
    margin-inline-start: calc(100% - 24px);
    width: 16px;
    height: 16px;
    border: 1px solid transparent;
    border-top-color: var(--highlight);
    border-radius: 50%;
    animation: spin 1s ease infinite; }

@keyframes spin {
  100% {
    transform: rotate(360deg); } }
  .search .logo {
    display: inline-block;
    height: 1rem;
    border: 0;
    padding: 0px 2px;
    margin: auto 0;
    margin-right: 3px;
    background: transparent;
    vertical-align: text-bottom;
    border-radius: 0.15rem; }

.noSelect {
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none; }

.noSelect:focus {
  outline: none !important; }

#search-results {
  background-color: var(--main-background, #fff);
  box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.2), 0 10px 10px -5px rgba(0, 0, 0, 0.2);
  border-radius: 0.25rem;
  margin-top: 0.25rem;
  position: absolute;
  z-index: 1000;
  width: 100%;
  max-width: 100%;
  padding-inline-start: 0;
  display: none;
  border: 1px solid var(--highlight); }
  #search-results:not(:empty) {
    display: block; }
  #search-results a {
    white-space: normal; }

#search-results li a:hover {
  color: var(--color-gray);
  border-radius: 2px;
  text-decoration: underline; }

.search ul li {
  line-height: 1.4rem;
  padding: 0.25rem 1rem 0.25rem 1rem; }

.search ul {
  padding-top: 1rem;
  padding-bottom: 1rem; }

.search .found:not(:empty) {
  display: block;
  padding: 3px;
  padding-left: 8px;
  border-left: 1px solid var(--font-color);
  opacity: 0.5;
