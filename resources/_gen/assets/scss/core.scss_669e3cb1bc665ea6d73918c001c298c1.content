html.writing #notice {
  margin: 1rem; }

html.writing body {
  background-image: none; }

html.writing article {
  font-family: 'New York';
  color: var(--darker-text); }

html.writing main {
  background-color: transparent; }

html.writing header.info .title {
  color: var(--darker-text);
  font-size: 50pt;
  font-weight: 900; }

html.writing header.info .meta {
  display: none; }

html.writing section.page {
  margin-top: 2rem;
  margin-bottom: 3rem; }

html.writing .note.subtitle {
  background-color: transparent; }

html.writing .dropcap {
  float: left;
  font-weight: 900;
  font-size: 75px;
  line-height: 60px;
  padding-top: 12px;
  padding-right: 16px;
  padding-left: 3px; }

html.writing h1 {
  margin-top: 4rem;
  margin-bottom: 2rem;
  font-size: 25pt;
  font-weight: 900; }

.shop-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));
  gap: 10px 10px;
  grid-auto-flow: row;
  grid-auto-rows: 1fr; }

.shop-card h1, .shop-card h2, .shop-card h3, .shop-card a, .shop-card p {
  margin: 0 !important;
  color: var(--font-color); }

.shop-card h2 {
  color: var(--color-gray); }

.shop-card a {
  color: var(--font-color) !important; }

.shop-title h1 {
  font-weight: 900 !important;
  position: absolute;
  margin: 5rem; }

.card-img {
  background: var(--main-background); }

.shop-card {
  background: var(--gray-light);
  padding: 1rem;
  border-radius: 5px;
  transition: box-shadow .3s; }

.shop-card:hover {
  box-shadow: 0 0 5px rgba(33, 33, 33, 0.2); }

.shop-item {
  justify-self: center;
  align-self: center; }

.search {
  position: relative;
  justify-items: end;
  border-bottom: 1px solid transparent;
  min-width: 10rem;
  grid-area: search; }
  .search input {
    background-color: var(--highlight-background);
    border: 1px solid var(--highlight);
    width: 100%;
    height: 35px;
    padding: 2px;
    box-sizing: border-box;
    border-radius: 5px;
    color: var(--color-gray); }
    .search input:required + .search-spinner {
      display: block; }
  .search .search-spinner {
    position: absolute;
    top: 0;
    margin: 8px;
    margin-inline-start: calc(100% - 24px);
    width: 16px;
    height: 16px;
    border: 1px solid transparent;
    border-top-color: var(--highlight);
    border-radius: 50%;
    animation: spin 1s ease infinite; }

@keyframes spin {
  100% {
    transform: rotate(360deg); } }
  .search .logo {
    display: inline-block;
    height: 1rem;
    border: 0;
    padding: 0px 2px;
    margin: auto 0;
    margin-right: 3px;
    background: transparent;
    vertical-align: text-bottom;
    border-radius: 0.15rem; }

.noSelect {
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none; }

.noSelect:focus {
  outline: none !important; }

#search-results {
  background-color: var(--main-background);
  box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.2), 0 10px 10px -5px rgba(0, 0, 0, 0.2);
  border-radius: .25rem;
  margin-top: 0.25rem;
  position: absolute;
  z-index: 11;
  width: -webkit-max-content;
  width: -moz-max-content;
  width: max-content;
  max-width: 100%;
  padding-inline-start: 0; }
  #search-results a {
    white-space: normal; }

#search-results li a:hover {
  color: var(--color-gray);
  border-radius: 2px;
  text-decoration: underline; }

.search ul li {
  line-height: 1.4rem;
  padding: 0.25rem 1rem 0.25rem 1rem; }

.search ul {
  padding-top: 1rem;
  padding-bottom: 1rem; }

.search .found:not(:empty) {
  display: block;
  padding: 3px;
  padding-left: 8px;
  border-left: 1px solid var(--font-color);
  opacity: 0.5;
  font-size: 80%; }

.search mark {
  background: var(--color-gray); }

.search small {
  opacity: 0.5;
  display: block; }

.search ul li {
  list-style: none; }

html {
  visibility: hidden;
  opacity: 0;
  transition: all .5s ease-in-out; }

